{
    "name": "brianfp/laracent",
    "type": "library",
    "description": "Centrifugo broadcaster for Lumen",
    "keywords": ["lumen", "centrifugo", "broadcaster"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Hnydiuk",
            "email": "o.hnydiuk@gmail.com"
        },
        {
            "name": "Brian Florian",
            "email": "brianflorianp@gmail.com"
        }
    ],
    "homepage": "https://github.com/AlexHnydiuk/laracent",
    "support": {
        "issues": "https://github.com/AlexHnydiuk/laracent/issues",
        "source": "https://github.com/AlexHnydiuk/laracent"
    },
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^7",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/broadcasting": "^7.0",
        "predis/predis": "^1.1",
        "lcobucci/jwt": "3.3.*"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^7|^8"
    },
    "autoload": {
        "psr-4": {
            "AlexHnydiuk\\Laracent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexHnydiuk\\Laracent\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexHnydiuk\\Laracent\\LaracentServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
